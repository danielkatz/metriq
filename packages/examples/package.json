{
  "name": "@metriq/examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:types": "tsc --noEmit",
    "lint": "eslint src",
    "bench": "vitest bench",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "metriq": "*"
  },
  "devDependencies": {
    "@types/node": "^22.10.1"
  }
}