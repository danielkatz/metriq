    "fastify": "^5.2.1",
    "metriq": "*",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2"
  }
}