{
  "name": "@metriq/examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "start": "tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:types": "tsc --noEmit",
    "lint": "eslint src",
    "bench": "vitest bench",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@metriq/express": "*",
    "express": "^4.21.2",
    "metriq": "*",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2"
  }
}
